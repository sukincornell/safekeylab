{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn"
  },
  "globals": {
    "AegisConfig": "readonly",
    "AegisAPI": "readonly",
    "AegisUtils": "readonly",
    "Chart": "readonly"
  }
}