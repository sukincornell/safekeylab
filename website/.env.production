# Aegis Shield - Production Environment Variables
# These values are used in production deployment

# API Configuration
AEGIS_API_URL=https://api.aegis-shield.com

# OAuth Configuration
GITHUB_CLIENT_ID=github-client-id-production
GOOGLE_CLIENT_ID=google-client-id-production

# Analytics
GA_TRACKING_ID=UA-XXXXXXXXX-X
MIXPANEL_TOKEN=your-mixpanel-token

# Feature Flags
ENABLE_BETA_FEATURES=false
ENABLE_ANALYTICS=true

# Security
SESSION_TIMEOUT_MINUTES=60
ENABLE_RATE_LIMITING=true
MAX_API_REQUESTS_PER_MINUTE=60

# Environment
ENVIRONMENT=production